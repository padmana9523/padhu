import java.io.*;
import java.util.*;

public class Solution {

    static long power(int a, int b) {
        long result = 1;
        
        for (int i = 0; i < b; i++) {
            result *= a;
        }
        
        return result;
    }

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        int a = scanner.nextInt();
        int b = scanner.nextInt();
        
        System.out.println(power(a, b));
        
        scanner.close();
    }
}
