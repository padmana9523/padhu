import java.io.*;
import java.util.*;

public class Solutions{

    public static void main(String args[]) {
       Scanner s=new Scanner(System.in);
        int n;
        
        n=s.nextInt();
        if(n%2==0)
        {
            System.out.print("Even");
        }
        else
        {
            System.out.print("Odd");
        }
    }
}
