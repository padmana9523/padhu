import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int M;
        M=s.nextInt();
        if(M>=90)
        {
            System.out.print("A");
        }
        else if(M>=75)
        {
           System.out.print("B") ;
        }
        else if(M>=60)
        {
         System.out.print("c");   
        }
        else
        {
            System.out.print("Fail");
        }
    }
}
