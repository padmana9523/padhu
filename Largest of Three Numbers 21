import java.io.*;
import java.util.*;



public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int A = sc.nextInt();
        int B = sc.nextInt();
        int C = sc.nextInt();
        
        int largest = A; // assume A is the largest
        
        if (B > largest) {
            largest = B;
        }
        if (C > largest) {
            largest = C;
        }
        
        System.out.println(largest);
        
        sc.close();
    }
}
