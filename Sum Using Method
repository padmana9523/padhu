import java.io.*;
import java.util.*;

public class Solution {
    
    public static int sum(int a, int b) {
        return a + b;
    }

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        int a = scanner.nextInt();
        int b = scanner.nextInt();
        
        int result = sum(a, b);
        System.out.println(result);
        
    }
}
